{
  "name": "xtal-element",
  "version": "0.0.575",
  "description": "Some opinionated web component base classes.",
  "main": "XE.js",
  "module": "XE.js",
  "scripts": {
    "test": "playwright test",
    "serve": "node node_modules/may-it-serve/serve.js",
    "start": "es-dev-server --app-index demo/index.html --node-resolve --watch --open",
    "package-check": "package-check",
    "pub": "git commit -m \"...\" && git push && npm version patch && npm publish && git pull && git push",
    "safari": "npx playwright wk http://localhost:3030",
    "update": "ncu -u && npm install"
  },
  "keywords": [
    "web-components"
  ],
  "files": [
    "types.d.ts",
    "*.js"
  ],
  "typings": "types.d.ts",
  "exports": {
    ".": "./XE.js",
    "./XE.js": "./XE.js",
    "./src/XE.js": "./src/XE.js",
    "./src/XENON.js": "./src/XENON.js"
  },
  "dependencies": {
    "trans-render": "0.0.699"
  },
  "devDependencies": {
    "@playwright/test": "1.36.1",
    "may-it-serve": "0.0.5",
    "xtal-shell": "0.0.27"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/xtal-element.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/xtal-element/issues"
  },
  "homepage": "https://github.com/bahrus/xtal-element#readme"
}
