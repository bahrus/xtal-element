{
  "name": "xtal-element",
  "version": "0.0.534",
  "description": "Some opinionated web component base classes.",
  "main": "XE.js",
  "module": "XE.js",
  "scripts": {
    "test": "playwright test",
    "serve": "node node_modules/may-it-serve/serve.js",
    "start": "es-dev-server --app-index demo/index.html --node-resolve --watch --open",
    "package-check": "package-check",
    "pub": "git commit -m \"...\" && git push && npm version patch && npm publish && git pull && git push"
  },
  "keywords": [
    "web-components"
  ],
  "files": [
    "lib/*",
    "src/*",
    "types.d.ts"
  ],
  "typings": "types.d.ts",
  "exports": {
    ".": "./XE.js",
    "./XE.js": "./XE.js",
    "./src/XE.js": "./src/XE.js",
    "./src/XENON.js": "./src/XENON.js",
    "./lib/applyMixins.js": "./lib/applyMixins.js",
    "./lib/attr.js": "./lib/attr.js",
    "./lib/base-link-id.js": "./lib/base-link-id.js",
    "./lib/createNestedProp.js": "./lib/createNestedProp.js",
    "./lib/debounce.js": "./lib/debounce.js",
    "./lib/define.js": "./lib/define.js",
    "./lib/destruct.js": "./lib/destruct.js",
    "./lib/getDestructArgs.js": "./lib/getDestructArgs.js",
    "./lib/getProcessor.js": "./lib/getProcessor.js",
    "./lib/getSlicedPropDefs.js": "./lib/getSlicedPropDefs.js",
    "./lib/GroupedSiblings.js": "./lib/GroupedSiblings.js",
    "./lib/GroupedSiblingsWithRefs.js": "./lib/GroupedSiblingsWithRefs.js",
    "./lib/html.js": "./lib/html.js",
    "./lib/initInternals.js": "./lib/initInternals.js",
    "./lib/intersection.js": "./lib/intersection.js",
    "./lib/mergeProps.js": "./lib/mergeProps.js",
    "./lib/nudge.js": "./lib/nudge.js",
    "./lib/observeCssSelector.js": "./lib/observeCssSelector.js",
    "./lib/passAttrToProp.js": "./lib/passAttrToProp.js",
    "./lib/pinTheDOMToKeys.js": "./lib/pinTheDOMToKeys.js",
    "./lib/propUp.js": "./lib/propUp.js",
    "./lib/substrBefore.js": "./lib/substrBefore.js",
    "./lib/with-path.js": "./lib/with-path.js"
  },
  "dependencies": {
    "trans-render": "0.0.636"
  },
  "devDependencies": {
    "@playwright/test": "1.27.1",
    "@skypack/package-check": "0.2.2",
    "may-it-serve": "0.0.3",
    "xtal-shell": "0.0.27"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/xtal-element.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/xtal-element/issues"
  },
  "homepage": "https://github.com/bahrus/xtal-element#readme"
}
